<script type="text/javascript">
	jQuery.noConflict();
	(function($) {
		$(document).ready(function() {

			/* accept friends  */

			$(".add").click(function() {
				console.log(this);
				var url = $(this).attr("href");
				var link = this;
				$.get(url, function(data) {
					if (data = true) {
						$(link).html('<i class="icon-ok"> <!-- com --></i>');
						$(link).attr('disabled', true);
					}
				});
				return false;
			});

			/* remove friend */

			$(".remove").click(function() {
				console.log(this);
				var url = $(this).attr("href");
				var link = this;
				$.get(url, function(data) {
					if (data = true) {
						$(link).closest('li').remove();
					}
				});
				return false;
			});

		});
	})(jQuery);
</script>


